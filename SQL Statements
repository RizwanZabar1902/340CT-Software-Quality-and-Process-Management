CREATE TABLE staff (`ID` INT UNSIGNED NOT NULL AUTO_INCREMENT, `Title` VARCHAR(10), `FirstName` VARCHAR(50), `LastName` VARCHAR(50), `Nickname` VARCHAR(50), `Password` VARCHAR(25), `ConfirmPassword` VARCHAR(50), PRIMARY KEY (`ID`));
